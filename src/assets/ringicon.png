import nospiedumsimg from './assets/nosp.png';
import namejsimg from './assets/namejs.png';
import lgredzeniimg from './assets/lgredzeni.png';

const Meistarklases = () =&gt; {
  return (
    &lt;div className="max-w-6xl mx-auto px-4 py-10"&gt;
      &lt;h1 className="text-3xl font-bold text-center mb-10"&gt;Meistarklases&lt;/h1&gt;

      &lt;div className="grid md:grid-cols-2 gap-12"&gt;
        {/* Nameja gredzena meistarklase */}
        &lt;div className="bg-white rounded shadow-lg p-10 flex flex-col md:flex-row items-center gap-10"&gt;
  &lt;img
    src={namejsimg}
    alt="Nameja gredzens"
    className="w-full md:w-1/2 rounded-lg"
  /&gt;
  &lt;div&gt;
    &lt;h2 className="text-2xl font-bold mb-4"&gt;Nameja gredzena meistarklase&lt;/h2&gt;
    &lt;p className="text-gray-700 text-base mb-4 leading-relaxed"&gt;
      Dalībniekiem ir iespēja meistara vadībā izgatavot savu sudraba Nameja gredzenu.
    &lt;/p&gt;
    &lt;ul className="text-gray-600 text-sm space-y-1"&gt;
      &lt;li&gt; Ilgums: 2–3 stundas&lt;/li&gt;
      &lt;li&gt; Cena: 100 € (1 pers.), 320 € (4 pers.)&lt;/li&gt;
      &lt;li&gt; Nepieciešama iepriekšēja rezervācija&lt;/li&gt;
    &lt;/ul&gt;
  &lt;/div&gt;
&lt;/div&gt;
        {/* Kulons ar pirkstu nospiedumu */}
        &lt;div className="bg-white rounded shadow p-6 flex flex-col md:flex-row items-center gap-6"&gt;
          &lt;img
            src={nospiedumsimg}
            alt="Kulons ar pirksta nospiedumu"
            className="w-full md:w-1/2 rounded"
          /&gt;
          &lt;div&gt;
            &lt;h2 className="text-2xl font-bold mb-4"&gt;Kulons ar pirkstu nospiedumu&lt;/h2&gt;
            &lt;p className="text-gray-700 mb-4 leading-relaxed"&gt;
              Radiet unikālu kulonu ar savu vai tuvinieka pirksta nospiedumu.
            &lt;/p&gt;
            &lt;ul className="text-gray-600 text-sm space-y-1"&gt;
            &lt;li&gt; Cena: 100 € (1 pers.), 180 € (2 pers.)&lt;/li&gt;
            &lt;li&gt; Nepieciešama iepriekšēja rezervācija&lt;/li&gt;
         &lt;/ul&gt;
          &lt;/div&gt;
        &lt;/div&gt;

        {/* Laulību gredzeni */}
        &lt;div className="bg-white rounded-xl shadow-lg p-10 flex flex-col md:flex-row items-center gap-10"&gt;
  &lt;img
    src={lgredzeniimg}
    alt="Laulību gredzeni"
    className="w-full md:w-1/2 rounded-lg shadow-md"
  /&gt;
  &lt;div&gt;
    &lt;h2 className="text-2xl font-bold mb-4 text-[#1c203c]"&gt;Laulību gredzeni&lt;/h2&gt;
    &lt;p className="text-gray-700 text-base mb-4 leading-relaxed"&gt;
      Divu cilvēku savienība ir īpašs mirklis — savijas saknes, savijas dzīves. Tas jānosvin ar gredzeniem, kuros atklājas jūsu abu stāsts, kuros iekalta kopīga nākotne. Stāstiet savu ieceri mums, un mēs palīdzēsim to pārvērst gredzenos, kurus nest ar mīlestību un lepnumu.
    &lt;/p&gt;
    &lt;ul className="text-gray-600 text-sm space-y-1"&gt;
      &lt;li&gt; Ilgums: ~30 minūtes&lt;/li&gt;
      &lt;li&gt; Cena: 10 € (bronza), 20 € (sudrabs)&lt;/li&gt;
    &lt;/ul&gt;
  &lt;/div&gt;
&lt;/div&gt;

      &lt;div className="text-center mt-10 text-gray-600"&gt;
         Norises vieta: Siguldas pils kvartāls, Pils iela 16, Sigulda&lt;br /&gt;
        E-pasts: info@balturotas.lv |  Tālrunis: +371 29295142
      &lt;/div&gt;

      &lt;div className="h-32" /&gt; {/* Lai būtu skrollējams apakšā */}
    &lt;/div&gt;
  );
